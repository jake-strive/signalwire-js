{
  "name": "@signalwire/core",
  "description": "Shared code for the SignalWire JS SDK",
  "author": "SignalWire Team <open.source@signalwire.com>",
  "license": "MIT",
  "version": "3.0.0-beta.0",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "esmodule": "./dist/index.modern.mjs",
  "type": "commonjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "default": "./dist/index.modern.mjs"
    }
  },
  "module": "dist/index.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "signalwire",
    "audio",
    "video",
    "relay",
    "sip"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/signalwire/signalwire-js"
  },
  "bugs": {
    "url": "https://github.com/signalwire/signalwire-js/issues"
  },
  "homepage": "https://github.com/signalwire/signalwire-js/tree/master/packages/core",
  "scripts": {
    "start": "microbundle watch -f modern,esm --define process.env.NODE_ENV=development --tsconfig tsconfig.build.json",
    "build": "pkg-typings --pre-run && microbundle -f modern,esm,cjs --define process.env.NODE_ENV=production --tsconfig ./tsconfig.build.json --target node && pkg-typings --after-run",
    "test": "NODE_ENV=test jest",
    "prepare": "npm run build"
  },
  "dependencies": {
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.6.0",
    "@types/uuid": "^8.3.0",
    "eventemitter3": "4.0.7",
    "jest-websocket-mock": "^2.2.1",
    "loglevel": "^1.7.1",
    "mock-socket": "^9.0.3",
    "redux-saga": "^1.1.3",
    "redux-saga-test-plan": "^4.0.1",
    "strict-event-emitter-types": "^2.0.0"
  },
  "types": "dist/core/src/index.d.ts"
}
